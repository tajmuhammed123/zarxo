					</div>

					<!-- Icon header -->
					<div class="wrap-icon-header flex-w flex-r-m">
						<!-- <div class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 js-show-modal-search">
							<i class="zmdi zmdi-search"></i>
						</div> -->

						<a href="/wallet" class="dis-block icon-header-item cl2 hov-cl1 trans-04 p-r-11 p-l-10">
							<i class="zmdi zmdi-filter-list"></i>
						</a>

						<div class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 js-show-cart">
							<i class="zmdi zmdi-shopping-cart"></i>
						</div>

						<a href="/wishlist" class="dis-block icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11">
							<i class="zmdi zmdi-favorite-outline"></i>
						</a>
						<% if(session!=null){ %>
							<a href="/userprofile" class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11">
								<span class="iconify" data-icon="gg:profile"></span></a>
							<% }else{ %>
								<a href="/login" class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11">
									<span class="iconify" data-icon="gg:profile"></span></a>
								<% } %>

